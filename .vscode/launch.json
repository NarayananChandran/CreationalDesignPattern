    "version": "0.2.0",
    "configurations": [
        {
            "name": "ts-node",
            "type": "node",
            "request": "launch",
            "args": [
                "${relativeFile}"
            ],
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {"REDIS_HOST_NAME": "127.0.0.1","REDIS_PORT" : "6379","WORKER_NAME":"sum","WORKER_VERSION" : "1.0"}
        }
    ]
}